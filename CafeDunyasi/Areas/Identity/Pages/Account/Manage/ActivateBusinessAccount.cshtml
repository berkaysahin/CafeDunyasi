@page
@model ActivateBusinessAccountModel
@{
    ViewData["Title"] = "Activate Business Account";
    ViewData["ActivePage"] = ManageNavPages.ActivateBusinessAccount;
}

<h4>@ViewData["Title"]</h4>
<partial name="_StatusMessage" model="Model.StatusMessage" />
<div class="row">
    <div class="col-md-6">
        <form id="profile-form" method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <button id="update-profile-button" type="submit" class="btn btn-warning">Activate</button>
        </form>
    </div>
</div>

@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}